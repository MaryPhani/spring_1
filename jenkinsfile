pipeline {
    agent any
    environment {
    AWS_ACCESS_KEY_ID = "aws"
    AWS_SECRET_ACCESS_KEY = "pass"
    }
    stages {
        stage('build') {
        steps {
            echo "Phani"
        }
    }
    }
        stage('Dockerbuild'){
           steps {
               sh 'mvn clean package'
               sh 'docker build -t Chintu .'
      }
   }
        stage('Snyk Test') {
            steps {
                echo 'Snyk Testing...'
                snykSecurity (
                    projectName: 'Snyk_security_tool', 
                    snykInstallation: 'snyk@latest', 
                    snykTokenId: 'snyk_api',
                    failOnIssues: false
                )
            }
        }
  }
